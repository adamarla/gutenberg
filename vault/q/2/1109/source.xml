<?xml version="1.0" encoding="UTF-8"?>
<question xmlns="http://www.gradians.com" chapterId="19">
  <statement>
    <tex>\text{If }
A=\left[\begin{array}{ccc}
  2 &amp; 0 &amp; 1 \\
  2 &amp; 1 &amp; 3 \\
  1 &amp; -1 &amp; 0
\end{array}\right], \text{find } A^2-5A+16I.</tex>
  </statement>
  <step>
    <options>
      <tex>A=\left[\begin{array}{ccc}
  2 &amp; 0 &amp; 1 \\
  2 &amp; 1 &amp; 3 \\
  1 &amp; -1 &amp; 0
\end{array}\right]\implies
A^2=\left[\begin{array}{ccc}
  5 &amp; -1 &amp; 2 \\ 9 &amp; -2 &amp; 5 \\ 0 &amp; -1 &amp; -2
\end{array}\right]</tex>
      <tex correct="false">A=\left[\begin{array}{ccc}
  2 &amp; 0 &amp; 1 \\
  2 &amp; 1 &amp; 3 \\
  1 &amp; -1 &amp; 0
\end{array}\right]\implies
A^2=\left[\begin{array}{ccc}
  4 &amp; 0 &amp; 1 \\
  4 &amp; 1 &amp; 9 \\
  1 &amp; 1 &amp; 0
\end{array}\right]</tex>
    </options>
    <reason>
      <tex>\begin{align}
A^2=A\cdot A&amp;=\left[\begin{array}{ccc}
  2 &amp; 0 &amp; 1 \\
  2 &amp; 1 &amp; 3 \\
  1 &amp; -1 &amp; 0
\end{array}\right]\cdot
\left[\begin{array}{ccc}
  2 &amp; 0 &amp; 1 \\
  2 &amp; 1 &amp; 3 \\
  1 &amp; -1 &amp; 0
\end{array}\right] \\
&amp;=\left[\begin{array}{ccc}
  4+0+1 &amp; 0+0-1 &amp; 2+0+0 \\
  4+2+3 &amp; 0+1-3 &amp; 2+3+0 \\
  2-2+0 &amp; 0-1+0 &amp; 1-3+0
\end{array}\right] \\
&amp;=\left[\begin{array}{ccc}
  5 &amp; -1 &amp; 2 \\ 9 &amp; -2 &amp; 5 \\ 0 &amp; -1 &amp; -2
\end{array}\right]
\end{align}
</tex>
    </reason>
    <skills>
      <skill id="86"/>
    </skills>
  </step>
  <step>
    <options>
      <tex>\begin{align}
A^2-5A&amp;=\left[\begin{array}{ccc} 
  5 &amp; -1 &amp; 2 \\ 9 &amp; -2 &amp; 5 \\ 0 &amp; -1 &amp; -2 
\end{array}\right]
-5\left[\begin{array}{ccc} 
  2 &amp; 0 &amp; 1 \\ 2 &amp; 1 &amp; 3 \\ 1 &amp; -1 &amp; 0 
\end{array}\right]\\
&amp;=\left[\begin{array}{ccc}
  -5 &amp; -1 &amp; -3 \\ -1 &amp; -7 &amp; -10 \\ -5 &amp; 4 &amp; -2
\end{array}\right]
\end{align}
</tex>
      <tex correct="false">\begin{align}
A^2-5A&amp;=\left[\begin{array}{ccc} 
  5 &amp; -1 &amp; 2 \\ 9 &amp; -2 &amp; 5 \\ 0 &amp; -1 &amp; -2 
\end{array}\right]
-5\left[\begin{array}{ccc} 
  2 &amp; 0 &amp; 1 \\ 2 &amp; 1 &amp; 3 \\ 1 &amp; -1 &amp; 0 
\end{array}\right] \\
&amp;=\left[\begin{array}{ccc}
  3 &amp; -1 &amp; 1 \\ -1 &amp; -7 &amp; -10 \\ -1 &amp; 0 &amp; -2
\end{array}\right]
\end{align}
</tex>
    </options>
    <reason>
      <tex>\begin{align}
A^2-5A&amp;=\left[\begin{array}{ccc} 
  5 &amp; -1 &amp; 2 \\ 9 &amp; -2 &amp; 5 \\ 0 &amp; -1 &amp; -2 
\end{array}\right]
-5\left[\begin{array}{ccc} 
  2 &amp; 0 &amp; 1 \\ 2 &amp; 1 &amp; 3 \\ 1 &amp; -1 &amp; 0 
\end{array}\right] \\
&amp;=\left[\begin{array}{ccc} 
  5 &amp; -1 &amp; 2 \\ 9 &amp; -2 &amp; 5 \\ 0 &amp; -1 &amp; -2 
\end{array}\right]
-\left[\begin{array}{ccc} 
  10 &amp; 0 &amp; 5 \\ 10 &amp; 5 &amp; 15 \\ 5 &amp; -5 &amp; 0 
\end{array}\right] \\
&amp;=\left[\begin{array}{ccc}
  -5 &amp; -1 &amp; -3 \\ -1 &amp; -7 &amp; -10 \\ -5 &amp; 4 &amp; -2
\end{array}\right]
\end{align} \\

</tex>
    </reason>
    <skills>
      <skill id="86"/>
    </skills>
  </step>
  <step>
    <options>
      <tex>\begin{align}
(A^2-5A)+16I&amp;=\left[\begin{array}{ccc}
  -5 &amp; -1 &amp; -3 \\ -1 &amp; -7 &amp; -10 \\ -5 &amp; 4 &amp; -2
\end{array}\right]+\left[\begin{array}{ccc}
  16 &amp; 0 &amp; 0 \\ 0 &amp; 16 &amp; 0 \\ 0 &amp; 0 &amp; 16
\end{array}\right]
\end{align}
</tex>
      <tex correct="false">\begin{align}
(A^2-5A)+16I&amp;=\left[\begin{array}{ccc}
  -5 &amp; -1 &amp; -3 \\ -1 &amp; -7 &amp; -10 \\ -5 &amp; 4 &amp; -2
\end{array}\right]+\left[\begin{array}{ccc}
  16 &amp; 16 &amp; 16 \\ 16 &amp; 16 &amp; 16 \\ 16 &amp; 16 &amp; 16
\end{array}\right]
\end{align}
</tex>
    </options>
    <reason>
      <tex>\begin{align}
(A^2-5A)+16I&amp;=\left[\begin{array}{ccc}
  -5 &amp; -1 &amp; -3 \\ -1 &amp; -7 &amp; -10 \\ -5 &amp; 4 &amp; -2
\end{array}\right]+16\overbrace{\left[\begin{array}{ccc}
  1 &amp; 0 &amp; 0 \\ 0 &amp; 1 &amp; 0 \\ 0 &amp; 0 &amp; 1
\end{array}\right]}^\text{Identity Matrix}\\
&amp;=\left[\begin{array}{ccc}
  -5+16 &amp; -1+0 &amp; -3+0 \\ -1+0 &amp; -7+16 &amp; -10+0 \\ -5+0 &amp; 4+0 &amp; -2+16
\end{array}\right] \\
=&amp;\left[\begin{array}{ccc}
  11 &amp; -1 &amp; -3 \\ -1 &amp; 9 &amp; -10 \\ -5 &amp; 4 &amp; 14
\end{array}\right]
\end{align}</tex>
    </reason>
    <skills>
      <skill id="98"/>
    </skills>
  </step>
  <choices>
    <tex correct="false">\left[\begin{array}{ccc}
  0 &amp; 0 &amp; 1 \\
